<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.virtualfluidlab">

    <application
        android:allowBackup="true"
        android:icon="@drawable/app_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/app_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MetaCenter"></activity>
        <activity android:name=".VNotch" />
        <activity android:name=".LosesInPipes" />
        <activity android:name=".ReynoldsNumber" />
        <activity android:name=".CenterOfPressure" />
        <activity android:name=".WindTunnel" />
        <activity
            android:name=".Pitot_Tube"
            android:screenOrientation="portrait" />
        <activity
            android:name=".MCQs"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ActionMenu"
            android:screenOrientation="portrait" />
        <activity
            android:name=".Bernoulli"
            android:configChanges="keyboardHidden|orientation"
            android:screenOrientation="portrait" />
        <activity
            android:name=".ListView"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar" />
        <activity
            android:name=".MainActivity"
            android:screenOrientation="portrait"
            android:theme="@style/Theme.AppCompat.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>